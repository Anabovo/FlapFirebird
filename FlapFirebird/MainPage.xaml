<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FlapFirebird.MainPage">

    
        <Grid> 
        <Image x:Name="ImagemFundo"
               Resources="fundoazul.jpg"
               HorizontalOptions="Fill"
               VerticalOptions="Fill"/>

        <Image x:Name="ImagemChao"
               Resources="chao.jpg"
               HorizontalOptions="Fill"
               VerticalOptions="End"/>
        
        <Image x:Name="ImagemCanoAlto"
               Resources="canoalto.png"
               HorizontalOptions="End"
               VerticalOptions="Start"/>

        <Image x:Name="ImagemCanoBaixo"
               Resources="canobaixo.png"
               HorizontalOptions="End"
               VerticalOptions="End"/>

        <Image x:Name="ImagemFirebird"
               Resources="firebird.png"
               HorizontalOptions="Start"
               HeightRequest="50"
               WidthRequest="50"/>



        </Grid>

</ContentPage>
